<?xml version="1.0" encoding="utf-8" ?>
<profile defaultProvider="DefaultProfileProvider" enabled="true" >
  <providers>
    <add name="DefaultProfileProvider"
         type="System.Web.Providers.DefaultProfileProvider, System.Web.Providers, Version=1.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35"
         connectionStringName="DefaultConnection"
         applicationName="/" />
  </providers>
  <properties>
    <group name="HtmlEscape">
      <!-- HtmlEscape -->
      <add name="TabSize" type="System.Int32" defaultValue="8" />
      <add name="Mode" type="System.String" defaultValue="Simple" />
    </group>
    <group name="SqlEnclosure">
      <!-- SqlEnclosure -->
      <add name="ReplaceChars" type="System.String" defaultValue="@@" />
      <add name="Pattern" type="System.String" defaultValue="strSQL = strSQL &amp; &quot;@@&quot; &amp; vbCrLf"/>
    </group>
  </properties>
</profile>
